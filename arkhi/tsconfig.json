{
	"compilerOptions": {
		"outDir": "./dist/",
		"declaration": true,
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "Node",
		"types": ["vite/client"],
		"strict": true,
		"noEmitOnError": false,
		"jsx": "react",
		"rootDir": "./",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"lib": ["DOM", "DOM.Iterable", "ES2021"]
	},
	"exclude": ["node_modules", "**/dist/"]
}
