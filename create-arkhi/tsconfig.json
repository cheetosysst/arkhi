{
	"compilerOptions": {
		"strict": true,
		"module": "ES2020",
		"target": "ES2020",
		"moduleResolution": "Node",
		"lib": ["ESNext"],
		"skipLibCheck": true,
		"esModuleInterop": true,
		"baseUrl": ".",
		"outDir": "dist",
		"paths": {
			"#/*": ["./*"]
		}
	},
	"ts-node": {
		"transpileOnly": true,
		"esm": true
	}
}
